{
    "name": "anourvalar/eloquent-journal",
    "description": "Laravel Journal (log & metric)",
    "keywords": ["anourvalar", "laravel", "eloquent", "journal"],
    "homepage": "https://github.com/AnourValar/eloquent-journal",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "anourvalar/eloquent-request": "^2.10",
        "anourvalar/eloquent-validation": "^3.16",
        "anourvalar/laravel-atom": "^2.23",
        "anourvalar/laravel-form": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0|^10.0",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.26",
        "squizlabs/php_codesniffer": "^3.7",
        "psalm/plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {"AnourValar\\EloquentJournal\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"AnourValar\\EloquentJournal\\Tests\\": "tests/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnourValar\\EloquentJournal\\Providers\\AnourValarEloquentJournalServiceProvider"
            ]
        }
    }
}
